{
  "name": "lavamusic",
  "description": "Lavamusic is a powerful Discord Music Bot you need in your server.",
  "repository": "https://github.com/brblacky/lavamusic",
  "logo": "https://media.discordapp.net/attachments/876035356460462090/888434725235097610/20210820_124325.png",
  "keywords": ["discord", "discordjs", "music"],
  "env": {    
    "MONGO_URI": {
     "description": "Your Mongo url",
     "value": "",
     "required": true
    },
    "NODE_HOST":{
      "description":"lavalink host",
      "value": "disbotlistlavalink.ml",
      "required": true
    },
    "NODE_ID":{
      "description":"lavalink ID",
      "value": "main",
      "required": true
    },
    "NODE_PORT":{
      "description":"lavalink port",
      "value": "443",
      "required": true
    },
    "NODE_PASSWORD":{
      "description":"lavalink password",
      "value": "LAVA",
      "required": true
    },
    "NODE_SECURE":{
      "description":"lavalink secure (true/false)",
      "value": "true",
      "required": true
    },
    "PREFIX":{
      "description": "your prefix",
      "value": "!",
      "required": true
     },
    "COLOR":{
      "description": "embed colour code hex",
      "value": "#303236",
      "required": false
     },
    "LOGS":{
      "description": "Channel ID",
      "required": true
     },
    "OWNERID":{
      "description": "Your Discord User ID",
      "required": false
     },
    "TOKEN": {
      "description": "Your bot's bot token.",
      "required": true
    },
    "LOCALE": {
      "description": "Default bot's language, see src/resources/locales for list of supported locales",
      "required": false
    }
  }
}
